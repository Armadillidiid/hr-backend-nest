    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@prisma/client": "5.13.0",
    "fastify": "^4.27.0",
    "pino-pretty": "^11.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.17",
    "@types/jest": "^29.5.12",
